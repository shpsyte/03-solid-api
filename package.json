{
  "name": "03-solid-api",
  "version": "1.0.0",
  "description": "GymPass style app.",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"tsx watch src/server.ts\"",
    "build": "tsup src --out-dir build",
    "start": "node build/server.js",
    "test": "vitest run --dir src/__test__/",
    "test:watch": "vitest --dir src/__test__/unit",
    "test:create-prisma-env": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-env": "npm link vitest-environment-prisma",
    "pretest:e2e": "run-s test:create-prisma-env test:install-prisma-env",
    "test:e2e": "vitest run --dir src/__test__/unit",
    "test:coverage": "vitest --coverage --dir src/__test__/unit",
    "test:ui": "vitest --ui",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint": "eslint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.1.0",
    "@types/node": "20.5.0",
    "@vitest/coverage-c8": "0.28.5",
    "@vitest/ui": "0.28.5",
    "npm-run-all": "4.1.5",
    "prisma": "5.1.1",
    "tsup": "7.2.0",
    "tsx": "3.12.7",
    "typescript": "5.1.6",
    "vite-tsconfig-paths": "4.0.5",
    "vitest": "0.28.5"
  },
  "dependencies": {
    "@fastify/jwt": "7.2.0",
    "@prisma/client": "5.1.1",
    "@types/bcryptjs": "2.4.2",
    "bcryptjs": "2.4.3",
    "concurrently": "8.2.0",
    "datejs": "1.0.0-rc3",
    "dayjs": "1.11.9",
    "dotenv": "16.3.1",
    "fastify": "4.21.0",
    "zod": "3.22.1"
  }
}
